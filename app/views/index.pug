extends layouts/default
block prepend link
  - var selected = 'HOME'
block title
  title='Genial Info | Home'
block content

  // Company List  
  section
    .companylistcontainer
     .row.text-white.d-relative.mb-5
      .col-sm-2
       img(src='https://www.wildnettechnologies.com/wp-content/uploads/2018/06/Air_India_Express_wildnet-cleint-gray-2.png' width = '70px' height = '50px')
      .col-sm-2
        img(src='https://www.wildnettechnologies.com/wp-content/uploads/2018/05/wildnet-home-24-1.png' width = '80px' height = '50px')
      .col-sm-2
        img(src='https://www.wildnettechnologies.com/wp-content/uploads/2018/11/adani_transparent.png' width = '80px' height = '50px')


  // Services 
  section
   p.numberText  We begin to serve you before you know it
   p.numberText(style = 'color : orange')  Our Solution to your Imaginations
  .container-fluid
  .row.port-page
    .gallery_product.col-lg-4.col-md-4.col-sm-4.col-xs-6.filter.hdpe.zoom
      img.img-responsive.center-block.port-image(src='/images/mobile-phone.jpg' width = '365px' height = '365px')
      .image-text
        h1| Mobile App
    .gallery_product.col-lg-4.col-md-4.col-sm-4.col-xs-6.filter.sprinkle.zoom
      img.img-responsive.center-block.port-image(src='/images/earth.jpg' width = '365px' height = '365px')
      .image-text
        h1| Web App
    .gallery_product.col-lg-4.col-md-4.col-sm-4.col-xs-6.filter.hdpe.zoom
      img.img-responsive.center-block.port-image(src='/images/seo.jpg' width = '365px' height = '365px')
      .image-text
        h1| SEO
    .gallery_product.col-lg-4.col-md-4.col-sm-4.col-xs-6.filter.irrigation.zoom
      img.img-responsive.center-block.port-image(src='/images/website.jpg' width = '365px' height = '365px')
      .image-text
        h1| Website Development
    .gallery_product.col-lg-4.col-md-4.col-sm-4.col-xs-6.filter.spray.zoom
      img.img-responsive.center-block.port-image(src='http://fakeimg.pl/365x365/')
    .gallery_product.col-lg-4.col-md-4.col-sm-4.col-xs-6.filter.irrigation.zoom
      img.img-responsive.center-block.port-image(src='http://fakeimg.pl/365x365/')


  // Counting             
  section.counting
    .containerproject         
     .typewriter
       p.numberText  it is the NUMBER that keep us motivated
       .row.justify-content-md-center.px-5.t-5.number
          span.numscroller.col.col-lg-2(data-min='1', data-max='10', data-delay='1', data-increment='1')
          span.numscroller.col.col-lg-2(data-min='1', data-max='10', data-delay='1', data-increment='1')
          span.numscroller.col.col-lg-2(data-min='1', data-max='10', data-delay='1', data-increment='1')
          span.col.col-lg-2(style='font-size : 4px ; color : white' )
           svg(height='80', viewbox='0 50 200 200' , )
            path(fill='none', stroke='#333333', stroke-width='5', d='M100,100 \
             C200,0 200,200 100,100\
             C0,0 0,200 100,100z')
       .row.justify-content-md-center.px-5.t-5.text
         p.col.col-lg-2 Technocrats 
         p.col.col-lg-2 Projects
         p.col.col-lg-2 Clients
         p.col.col-lg-2 Happy Moments
  

  

  // Recent Blogs 
